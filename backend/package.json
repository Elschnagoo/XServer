{
  "name": "@elschnagoo/xserver",
  "version": "1.13.0",
  "description": "GrandLineX Full Stack Demo Project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "BSD-3-Clause",
  "scripts": {
    "buildprep": "tsc",
    "start": "node dist/run.js",
    "start-dev": "ts-node src/run.ts",
    "debug": "ts-node tests/run.ts",
    "lint": "eslint src",
    "test": "jest --runInBand --ci --collectCoverage   --coverageDirectory=\"./coverage\"  --reporters=default --reporters=jest-junit",
    "test-coverage": "npm run buildTestApi && npm run test",
    "makeDocs": "typedoc",
    "buildApi": "npm run buildprep && npm run buildSpecMain && npm --prefix ./gen run build",
    "buildTestApi": "npm run buildSpecDev && npm --prefix ./gen run build",
    "makeSpec": "swagger-mate",
    "serveSpec": "swagger-mate --serve",
    "buildSpecMain": "swagger-mate --build --main",
    "buildSpecDev": "swagger-mate --build --dev"
  },
  "dependencies": {
    "@grandlinex/bundle-elogger": "1.0.2",
    "@grandlinex/bundle-postgresql": "1.0.0",
    "@grandlinex/kernel": "1.0.0",
    "axios": "1.7.2",
    "express": "4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.0",
    "moment": "2.30.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/jest": "29.5.12",
    "@types/node": "22.0.0",
    "@types/pg": "8.11.6",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "html-webpack-plugin": "5.6.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "3.3.3",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  },
  "glx": {
    "kernel": "dist/XKernel.js"
  }
}
